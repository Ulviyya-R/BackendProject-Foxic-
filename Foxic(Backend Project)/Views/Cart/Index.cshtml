	<div class="cart-table-prd cart-table-prd--head py-1 d-none d-md-flex">
		<div class="cart-table-prd-image text-center">
			Image
		</div>
		<div class="cart-table-prd-content-wrap">
			<div class="cart-table-prd-info">Name</div>
			<div class="cart-table-prd-qty">Qty</div>
			<div class="cart-table-prd-price">Price</div>
			<div class="cart-table-prd-action">&nbsp;</div>
		</div>
	</div>
	@foreach(var item in Model.BasketItems)
	{
							<div class="cart-table-prd">
								<div class="cart-table-prd-image">
									<a asp-controller="Shop" asp-action="Detail" class="prd-img"><img class="lazyload fade-up" 
										src="~/assets/images/skins/fashion/products/@item.ProductSizeColor.Product.ProductImages.FirstOrDefault(pi=>pi.IsMain==true)?.Path" alt=""></a>
								</div>
								<div class="cart-table-prd-content-wrap">
									<div class="cart-table-prd-info">
										<div class="cart-table-prd-price">
											<div class="price-new">@item.ProductSizeColor.Product.Price</div>
										</div>
										<h2 class="cart-table-prd-name"><a asp-controller="Shop" asp-action="Detail">@item.ProductSizeColor.Product.Name</a></h2>
									</div>
									<div class="cart-table-prd-price-total">
										@*@item.Basket.TotalPrice*@
									</div>
								</div>
								<div class="cart-table-prd-action">
									<a class="cart-table-prd-remove" asp-controller="Cart" asp-action="RemoveCart" asp-route-id="@item.BasketId"><i class="icon-recycle"></i></a>
								</div>
							</div>

	}
				<div class="text-center mt-1"><a asp-controller="Cart" asp-action="ClearAllCart" asp-route-id="@Model.Id" class="btn btn--grey">Clear All</a></div>

</div>
